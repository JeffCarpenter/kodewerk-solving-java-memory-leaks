#!/bin/sh -e

#DEBHELPER#

# Remove cached files
rm -rf /var/cache/jetty6/*

case "$1" in
    purge)
	userdel jetty || true
        rm -rf /var/log/jetty6
    ;;

    remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
        # Nothing to do here
    ;;

    *)
        echo "$0 called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac
